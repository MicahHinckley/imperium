{
  "name": "replace",
  "globIgnorePaths": ["dependencies/*.spec.lua"],
  "tree": {
    "$className": "DataModel",
    "ReplicatedStorage": {
      "Shared": {
        "$path": "src/shared",

        "Imperium": {
          "$path": "dependencies/imperium/framework/shared",

          "Asink": {"$path": "dependencies/asink/src"},
          "Fmt": {"$path": "dependencies/fmt/src"},
          "t": {"$path": "dependencies/t/lib"},
          "Roact": {"$path": "dependencies/roact/src"},
          "Rodux": {"$path": "dependencies/rodux/src"},
          "RoactRodux": {"$path": "dependencies/roact-rodux/src"}
        }
      },
      "Imperium": {"$path": "dependencies/imperium/framework/Imperium.lua"},
      "import": {"$path": "dependencies/import/src"}
    },
    "ServerScriptService": {
      "Server": {
        "$path": "src/server"
      }
    },
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": {
          "$path": "src/client"
        }
      }
    }
  }
}